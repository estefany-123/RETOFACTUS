generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model client {
  identification Int @id
  dv Int?
  company String?
  trade_name String?
  names String
  address String
  email String
  phone String
  legal_organization_id String
  tribute_id String
  identification_document_id String
  municipality_id String
}

model product {
  code_reference String @id
  name String
  discount_rate Int
  price Int
  tax_rate String
  unit_measure_id Int
  standard_code_id Int
  is_excluded Int
  tribute_id Int
}